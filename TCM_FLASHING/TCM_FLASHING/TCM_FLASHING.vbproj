  <PropertyGroup>
    <PostBuildEvent>IF "$(ConfigurationName)"=="Debug" (
    SET ILREPACK_EXE="E:\p\EOL TOOL\Final Tool\TCM_FLASHING\packages\ILRepack.2.0.44\tools\ILRepack.exe"
    SET OUTPUT_DIR="$(TargetDir)"
    SET MAIN_EXE_NAME="$(TargetFileName)"
    SET TEMP_EXE_NAME="$(TargetName).original$(TargetExt)"
    SET LOG_FILE="%25OUTPUT_DIR%25ILRepack_Log.txt"

    ECHO Starting ILRepack merge for DEBUG configuration... &gt; "%25LOG_FILE%25"

    REM Change directory to the output folder. /D is crucial for cross-drive changes.
    pushd "%25OUTPUT_DIR%25" &gt;&gt; "%25LOG_FILE%25" 2&gt;&amp;1
    IF ERRORLEVEL 1 (
        ECHO ERROR: Failed to change directory to %25OUTPUT_DIR%25. &gt;&gt; "%25LOG_FILE%25"
        EXIT 1
    )

    REM Check if ILRepack.exe exists
    IF NOT EXIST %25ILREPACK_EXE%25 (
        ECHO ERROR: ILRepack.exe not found at %25ILREPACK_EXE%25 &gt;&gt; "%25LOG_FILE%25"
        EXIT 1
    )

    REM Rename the original EXE to a temporary name
    REM Use timeout to give Windows a moment to release file lock, if any.
    timeout /t 1 /nobreak &gt;nul
    move /Y "%25MAIN_EXE_NAME%25" "%25TEMP_EXE_NAME%25" &gt;&gt; "%25LOG_FILE%25" 2&gt;&amp;1
    IF ERRORLEVEL 1 (
        ECHO ERROR: Failed to move original EXE. File might be in use or missing. Check %25OUTPUT_DIR%25%25MAIN_EXE_NAME%25. &gt;&gt; "%25LOG_FILE%25"
        popd
        EXIT 1
    )

    REM Run ILRepack
    REM Using 'call' ensures we capture the exit code of ILRepack.exe properly
    call %25ILREPACK_EXE%25 /out:"%25MAIN_EXE_NAME%25" "%25TEMP_EXE_NAME%25" ^
    "CommInterface.dll" ^
    "J2534DotNet.dll" ^
    "Reprogramming_Dll.dll" ^
    /log:"%25LOG_FILE%25" /logpath:"%25OUTPUT_DIR%25"

    IF ERRORLEVEL 1 (
        ECHO ERROR: ILRepack exited with errors. Check %25LOG_FILE%25 for details. &gt;&gt; "%25LOG_FILE%25"
        popd
        EXIT 1
    ) ELSE (
        ECHO ILRepack merge completed successfully. &gt;&gt; "%25LOG_FILE%25"
        REM Optionally, delete the original DLLs if the merge was successful
        del "CommInterface.dll" &gt;&gt; "%25LOG_FILE%25" 2&gt;&amp;1
        del "J2534DotNet.dll" &gt;&gt; "%25LOG_FILE%25" 2&gt;&amp;1
        del "Reprogramming_Dll.dll" &gt;&gt; "%25LOG_FILE%25" 2&gt;&amp;1
    )

    REM Return to the original directory
    popd &gt;&gt; "%25LOG_FILE%25" 2&gt;&amp;1
)</PostBuildEvent>
